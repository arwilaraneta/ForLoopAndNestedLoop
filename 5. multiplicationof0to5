#include <iostream>
using namespace std;

int main() {
    // Loop over numbers from 0 to 5
    for (int i = 0; i <= 5; i++) {
        cout << "Multiplication Table for " << i << ":" << endl;
        // For each number, print the products for multipliers 0 to 10
        for (int j = 0; j <= 10; j++) {
            cout << i << " x " << j << " = " << i * j << endl;
        }
        cout << endl; // Add an empty line for readability between tables
    }
    return 0;
}
